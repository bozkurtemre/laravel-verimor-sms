{
    "name": "emrebbozkurt/laravel-verimor-sms",
    "description": "Verimor SMS for Laravel Framework",
    "homepage": "https://github.com/emrebbozkurt/laravel-verimor-sms",
    "license": "MIT",
    "keywords": [
        "laravel",
        "verimor",
        "sms"
    ],
    "authors": [
        {
            "name": "Emre Bozkurt",
            "email": "emre75kurt@gmail.com",
            "homepage": "https://github.com/emrebbozkurt"
        }
    ],
    "autoload": {
        "psr-4": {
            "Emrebbozkurt\\VerimorSms\\": "src"
        }
    },
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emrebbozkurt\\VerimorSms\\VerimorServiceProvider"
            ],
            "aliases": {
                "Verimor": "Emrebbozkurt\\VerimorSms\\Facades\\Verimor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
